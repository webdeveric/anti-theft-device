{
  "extends": "@webdeveric/eslint-config-babel",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "globals": {
    "makeCompiler": true
  }
}
